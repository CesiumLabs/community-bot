{
  "name": "community-bot",
  "version": "1.0.0",
  "description": "Community bot for Snowflake Studio",
  "main": "lib/index.js",
  "private": true,
  "scripts": {
    "build": "rimraf lib && tsc",
    "start": "cd lib && cross-env NODE_ENV=production node src/index.js",
    "dev": "cross-env NODE_ENV=development ts-node src/index.ts",
    "prettier": "prettier --write **/*.{js,ts,json,yml,yaml}",
    "format": "yarn prettier && git add . && git commit -m \"chore(prettier)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevSnowflake/community-bot.git"
  },
  "keywords": [
    "bot",
    "community",
    "discord"
  ],
  "author": "Snowflake Studio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DevSnowflake/community-bot/issues"
  },
  "homepage": "https://github.com/DevSnowflake/community-bot#readme",
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.6.1",
    "@types/node-fetch": "^2.5.10",
    "cross-env": "^7.0.3",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "common-tags": "^1.8.0",
    "discord.js": "discordjs/discord.js",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.12",
    "node-fetch": "^2.6.1",
    "turndown": "^7.0.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "erlpack": "discord/erlpack",
    "utf-8-validate": "^5.0.5",
    "zlib-sync": "^0.1.7"
  },
  "engines": {
    "node": ">=14.x.x"
  },
  "contributors": [
    "Snowflake107"
  ]
}
