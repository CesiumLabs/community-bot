{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "declaration": false,
        "outDir": "./lib",
        "strict": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "jsxFactory": "DiscordComponents.createComponent",
        "jsx": "react",
        "jsxFragmentFactory": "fragment"
    },
    "include": [
        "src/**/*"
    ]
}